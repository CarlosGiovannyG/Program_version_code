{
  "name": "programversion",
  "vendor": "easycolombia",
  "version": "1.1.0",
  "title": "Program Version",
  "description": "Componente que permite programar la salida  a producción de una versión de la aplicación de Jumbo",
  "mustUpdateAt": "2022-11-11",
  "scripts": {
    "postreleasy": "vtex publish"
  },
  "dependencies": {
    "vtex.styleguide": "9.x",
    "vtex.store-graphql": "2.x",
    "vtex.render-runtime": "8.x",
    "vtex.css-handles": "0.x",
    "easycolombia.profile-service-app": "3.x"
  },
  "builders": {
    "react": "3.x",
    "admin": "0.x",
    "messages": "1.x",
    "docs": "0.x"
  },
  "policies": [
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    },
    {
      "name": "vbase-read-write"
    },
    {
      "name": "graphql-query"
    },
    {
      "name": "vtex.store-graphql:resolve-graphql"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "easycolombia.vtexcommercestable.com.br",
        "path": "/api/dataentities/*"
      }
    }
  ],
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
